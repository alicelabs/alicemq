    "@babel/preset-react": "^7.25.9",
    "@babel/register": "^7.25.9",
    "babel-loader": "^9.2.1",
    "babel-jest": "^29.7.0",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "electron": "^28.3.3",
    "electron-packager": "^17.1.2",
    "express": "^4.21.2",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
