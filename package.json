{
  "name": "alicemq",
  "productName": "AliceMQ",
  "version": "1.0.1",
  "description": "RabbitMQ Visualizer",
  "main": "./main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "dev": "webpack serve --config webpack.dev.js",
    "reactdev": "webpack serve --config webpack.dev.js",
    "web": "webpack serve --config webpack.dev.js",
    "app": "webpack --config webpack.prod.js && electron .",
    "buildappmac": "electron-packager . --platform=darwin --arch=x64 --icon=./assets/icon/mac/alice-ribbon.icns --out=release-builds --overwrite",
    "buildappwin": "electron-packager . --platform=win32 --arch=ia32 --icon=./assets/icon/win/alice-ribbon.ico --out=release-builds",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:analyze": "webpack --config webpack.prod.js --env analyze",
    "buildapplinux": "electron-packager . --platform=linux --arch=x64 --icon=./assets/icon/linux/alice-ribbon.png --out=release-builds",
    "buildapp": "electron-packager .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx client server main.js",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx client server main.js --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alicelabs/alice.git"
  },
  "keywords": [],
  "author": "Anthony Valentin, Christian Niedermayer, Parker Allen, Siye Yu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alicelabs/alice/issues"
  },
  "homepage": "https://github.com/alicelabs/alice#readme",
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "amqplib": "^0.10.4",
    "core-js": "^3.39.0",
    "d3": "^7.9.0",
    "js-base64": "^3.7.8",
    "numeral": "^2.0.6",
    "regenerator-runtime": "^0.14.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/register": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "body-parser": "^1.20.3",
    "compression-webpack-plugin": "^11.1.0",
    "cookie-parser": "^1.4.7",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^16.4.7",
    "electron": "^35.7.5",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "express": "^4.21.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.11",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  }
}
